%{
#include <stdio.h>
%}

%%
[A-Z]+ {
    printf("Input is a CAPITAL: %s\n", yytext);
    return 0; // Return to stop further processing
}
.|\n {
    printf("Input is not a CAPTIAL\n");
    return 0; // Return to stop further processing
}
%%

int yywrap() {
    return 1;
}

int main() {
    yylex();
    return 0;
}
